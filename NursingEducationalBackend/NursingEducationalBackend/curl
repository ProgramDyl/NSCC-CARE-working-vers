# Curl command to submit Elimination data (Nested Format)
curl -X POST http://localhost:5232/api/patients/6/submit-data \
-H "Authorization: Bearer G1jOJtblhLw-STtQSLsXUQJtA8SJFPX3udOHqEm-Dso" \
-H "Content-Type: application/json" \
-d '{
  "form-elimination-1": {
    "IncontinentOfBladder": true,
    "IncontinentOfBowel": false,
    "DayOrNightProduct": "Adult Diaper - Day",
    "LastBowelMovement": "2025-04-26T08:00:00Z",
    "BowelRoutine": "Suppository PRN",
    "BladderRoutine": "Scheduled toileting q2h",
    "CatheterInsertion": false,
    "CatheterInsertionDate": null
  }
}'

